{
  "name": "agg-daily-tasks",
  "version": "1.0.0",
  "description": "Obsidian plugin that adds a button to insert aggregate previous daily note's tasks into the current note",
  "main": "main.js",
  "scripts": {
    "dev": "esbuild main.ts --bundle --external:obsidian --format=cjs --platform=node --outfile=main.js --watch",
    "build": "esbuild main.ts --bundle --external:obsidian --format=cjs --platform=node --outfile=main.js --minify",
    "test": "vitest --inspect-brk --no-file-parallelism",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "@vitest/ui": "^3.2.4",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.9",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "^4.7.4",
    "vitest": "^3.2.4"
  }
}
